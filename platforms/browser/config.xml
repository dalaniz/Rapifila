<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.helloworld" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>RapiFila</name>
    <description>
        Acceso a la aplicacion RapiFila.
    </description>
    <author email="support@phonegap.com" href="http://phonegap.com">
        PhoneGap Team
    </author>
    <icon src="rapifila2.png" />
    <icon gap:platform="android" gap:qualifier="ldpi" src="rapifila2.png" />
    <icon gap:platform="android" gap:qualifier="mdpi" src="rapifila2.png" />
    <icon gap:platform="android" gap:qualifier="hdpi" src="rapifila2.png" />
    <icon gap:platform="android" gap:qualifier="xhdpi" src="rapifila2.png" />
    <icon gap:platform="blackberry" src="rapifila2.png" />
    <icon gap:platform="blackberry" gap:state="hover" src="rapifila2.png" />
    <icon gap:platform="ios" height="57" src="rapifila2.png" width="57" />
    <icon gap:platform="ios" height="72" src="rapifila2.png" width="72" />
    <icon gap:platform="ios" height="114" src="rapifila2.png" width="114" />
    <icon gap:platform="ios" height="144" src="rapifila2.png" width="144" />
    <icon gap:platform="webos" src="rapifila2.png" />
    <icon gap:platform="winphone" src="rapifila2.png" />
    <icon gap:platform="winphone" gap:role="background" src="rapifila2.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-ldpi" src="rapifila2.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-mdpi" src="rapifila2.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-hdpi" src="rapifila2.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-xhdpi" src="rapifila2.png" />
    <gap:splash gap:platform="blackberry" src="rapifila2.png" />
    <gap:splash gap:platform="ios" height="480" src="rapifila2.png" width="320" />
    <gap:splash gap:platform="ios" height="960" src="rapifila2.png" width="640" />
    <gap:splash gap:platform="ios" height="1136" src="rapifila2.png" width="640" />
    <gap:splash gap:platform="ios" height="1024" src="rapifila2.png" width="768" />
    <gap:splash gap:platform="ios" height="768" src="rapifila2.png" width="1024" />
    <gap:splash gap:platform="winphone" src="res/screen/windows-phone/screen-portrait.jpg" />
    <gap:config-file parent="CFBundleShortVersionString" platform="ios">
        <string>100</string>
    </gap:config-file>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="permissions" value="none" />
    <preference name="orientation" value="default" />
    <preference name="target-device" value="universal" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="true" />
    <preference name="prerendered-icon" value="true" />
    <preference name="stay-in-webview" value="false" />
    <preference name="ios-statusbarstyle" value="black-opaque" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="show-splash-screen-spinner" value="true" />
    <preference name="auto-hide-splash-screen" value="true" />
    <preference name="disable-cursor" value="false" />
    <preference name="android-installLocation" value="auto" />
</widget>
